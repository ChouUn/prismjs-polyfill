{
  "name": "prismjs-polyfill",
  "version": "0.3.0",
  "description": "A polyfill for prism.js to import all languages and isolate various syntax themes because of global style pollution.",
  "main": "lib/index.js",
  "scripts": {
    "predev": "babel-node utils/generator.js --presets=es2015",
    "dev": "webpack --progress --colors --profile && babel src/ --out-dir lib/ --presets=es2015",
    "rls": "npm run dev && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ChouUn/prismjs-polyfill.git"
  },
  "keywords": [
    "prism",
    "prismjs",
    "polyfill",
    "highlight"
  ],
  "author": {
    "name": "ChouUn",
    "email": "ChouUnSoft@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChouUn/prismjs-polyfill/issues"
  },
  "homepage": "https://github.com/ChouUn/prismjs-polyfill#readme",
  "engines": {
    "node": ">=5.0"
  },
  "dependencies": {
    "prismjs": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^4.1.1",
    "webpack": "^2.2.1"
  }
}
