{
  "name": "prismjs-polyfill",
  "version": "0.2.0",
  "description": "A polyfill for prism.js to import all languages and isolate various syntax themes because of global style pollution.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:gene && npm run build:style && npm run build:lib",
    "build:gene": "babel-node src/generator.js --presets=es2015",
    "build:style": "webpack --config ./src/webpack.config.js --progress --colors --profile",
    "build:lib": "babel src/ --out-dir lib/ --presets=es2015 --ignore webpack.config.js,generator.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ChouUn/prismjs-polyfill.git"
  },
  "keywords": [
    "prism",
    "prismjs",
    "polyfill",
    "syntax"
  ],
  "author": {
    "name": "ChouUn",
    "email": "ChouUnSoft@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChouUn/prismjs-polyfill/issues"
  },
  "homepage": "https://github.com/ChouUn/prismjs-polyfill#readme",
  "engines": {
    "node": ">=5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^4.1.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "prismjs": "^1.6.0"
  }
}
